{
  "name": "cassidy-dark",
  "displayName": "Cassidy Dark Theme",
  "description": "A clean, modern, dark theme originally inspired by Monokai",
  "version": "0.2.6",
  "icon": "images/cassidy-dark-logo-512.png",
  "publisher": "jacobcassidy",
  "bugs": {
    "url": "https://github.com/jacobcassidy/vscode-cassidy-dark-theme/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jacobcassidy/vscode-cassidy-dark-theme"
  },
  "galleryBanner": {
    "color": "#292929",
    "theme": "dark"
  },
  "keywords": [
    "Theme",
    "Dark Theme",
    "Monokai",
    "Night Theme"
  ],
  "engines": {
    "vscode": "^1.64.2"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Cassidy Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/cassidy-dark-color-theme.json"
      }
    ],
    "grammars": [
      {
        "scopeName": "missing-css-syntax.injection",
        "path": "./syntaxes/missing-css-syntax-injection.json",
        "injectTo": [
          "source.css"
        ]
      },
      {
        "scopeName": "missing-scss-syntax.injection",
        "path": "./syntaxes/missing-scss-syntax-injection.json",
        "injectTo": [
          "source.css.scss"
        ]
      }
    ]
  }
}
