{
  "scopeName": "missing-scss-syntax.injection",
  "injectionSelector": "L:meta.property-list.scss, L:source.css.scss",
  "patterns": [
    {
      "include": "#missing-punctuation"
    },
    {
      "include": "#missing-combinator"
    },
    {
      "include": "#missing-properties"
    },
    {
      "include": "#missing-url-parameter"
    }
  ],
  "repository": {
    "missing-punctuation": {
      "patterns": [
        {
          "match": ",",
          "name": "punctuation.separator.list.comma.scss.cassidydark"
        }
      ]
    },
    "missing-combinator": {
      "patterns": [
        {
          "match": "(?<!\\/\\/.*)(>>|>|\\+|~)",
          "name": "keyword.operator.combinator.scss.cassidydark"
        }
      ]
    },
    "missing-properties": {
      "patterns": [
        {
          "match": "(?<!\\/\\/.*)(content|cursor|filter|font|mask)(?=:)",
          "name": "support.type.property-name.scss.cassidydark"
        }
      ]
    },
    "missing-url-parameter": {
      "patterns": [
        {
          "match": "(?<=url\\(\\s*)/",
          "name": "variable.parameter.url.scss.cassidydark"
        }
      ]
    }
  }
}
