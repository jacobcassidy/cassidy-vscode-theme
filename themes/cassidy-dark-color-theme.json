{
  "$schema": "vscode://schemas/color-theme",
  "name": "Cassidy Dark",
  "type": "dark",
  "author": "Jacob Cassidy",
  "colors": {
    //*** BASE COLORS ***
    "descriptionForeground": "#f5f5f5", // theme gray white
    "errorForeground": "#ff7f72", // theme red
    "focusBorder": "#ffffff1a", // white at 10% opacity
    "foreground": "#cccccc", // theme gray light
    "icon.foreground": "#a3a3a3", // theme gray medium
    "sash.hoverBorder": "#ffffff1a", // white at 10% opacity
    "widget.border": "#ffffff05", // white at 2% opacity
    "widget.shadow": "#1f1f1f", // 12% black

    //*** ACTIVITY BAR ***
    "activityBar.background": "#333333", // 20% black (match sidebar header)
    "activityBar.dropBorder": "#7a7a7a", // theme gray dark
    "activityBar.foreground": "#cccccc", // theme gray light
    "activityBar.inactiveForeground": "#7a7a7a", // theme gray dark
    "activityBar.border": "#292929", // theme gray black (match sidebar bg)
    "activityBarBadge.background": "#f4e4b8", // theme yellow
    "activityBarBadge.foreground": "#141414", // theme black-gray-black
    "activityBar.activeBorder": "#f4e4b880", // theme yellow at 50% opacity
    "activityBar.activeBackground": "#292929", // theme gray black (match sidebar bg)
    "activityBar.activeFocusBorder": "#3d3d3d", // theme gray-black-fade

    //*** BADGE COLORS ***
    "badge.foreground": "#f5f5f5", // theme gray white
    "badge.background": "#525252", // theme gray fade

    //*** BUTTON CONTROLS ***
    "button.background": "#317fff", // theme blue deep - example: search settings in settings page
    "checkbox.background": "#ffffff1a", // white at 10% opacity - example: Settings UI checkbox
    "checkbox.foreground": "#f5f5f5", // theme gray white

    //*** DIFF EDITOR ***
    "diffEditor.insertedTextBackground": "#9bce9033", //33 theme green at 20% opacity
    "diffEditor.removedTextBackground": "#ff7f7233", // theme red at 20% opacity
    "diffEditor.insertedLineBackground": "#9bce901a", // theme green at 10% opacity
    "diffEditor.removedLineBackground": "#ff7f721a", // theme red at 10% opacity
    "diffEditorGutter.insertedLineBackground": "#9bce901a", // theme green at 10% opacity
    "diffEditorGutter.removedLineBackground": "#ff7f721a", // theme red at 10% opacity
    "diffEditorOverview.insertedForeground": "#9bce904d", // theme green at 30% opacity - example: scrollbar diff indicator
    "diffEditorOverview.removedForeground": "#ff7f724d", // theme red at 30% opacity - example: scrollbar diff indicator

    //*** DROPDOWN CONTROLS ***
    "dropdown.background": "#3d3d3d", // theme gray-black-fade
    "dropdown.listBackground": "#383838", // 22% black (2 shades darker than dropdown.background)
    "dropdown.foreground": "#f5f5f5", // theme gray white

    //*** EDITOR COLORS ***
    // base
    "editor.background": "#242424", // 14% black
    "editor.foreground": "#cccccc", // theme gray light
    // bracket
    "editorBracketMatch.border": "#7a7a7a", // theme gray dark
    // cursor
    "editorCursor.background": "#3d3d3d", // theme gray-black-fade
    "editorCursor.foreground": "#a3a3a3", // theme gray medium
    // fold
    "editor.foldBackground": "#98bffe26", // theme blue at 15% opacity
    // ghost text
    "editorGhostText.foreground": "#a3a3a3", // theme gray medium
    "editorGhostText.background": "#333", // 20% black
    // gutter
    "editorGutter.addedBackground": "#9bce9099", // theme green at 60% opacity - example: scrollbar and number line bar diff indicators
    "editorGutter.deletedBackground": "#ff7f7299", // theme red at 60% opacity
    "editorGutter.foldingControlForeground": "#7a7a7a", // theme gray dark
    "editorGutter.modifiedBackground": "#f4e4b8cc", // theme yellow at 80% opacity
    // lightbulb
    "editorLightBulb.foreground": "#ffe294", // theme yellow deep
    "editorLightBulbAutoFix.foreground": "#98bffe", // theme blue
    // indent guide
    "editorIndentGuide.background": "#3d3d3d", // theme gray-black-fade
    "editorIndentGuide.activeBackground": "#525252", // theme gray fade
    "editorWhitespace.foreground": "#3d3d3d", // theme gray-black-fade
    // line
    "editor.lineHighlightBackground": "#1a1a1a", // 10% black (4 shades darker than editor bg)
    // line number
    "editorLineNumber.foreground": "#424242", // 26% black
    "editorLineNumber.activeForeground": "#a3a3a3", // theme gray medium
    // link
    "editorLink.activeForeground": "#f4e4b8", // theme
    // match
    "editor.findMatchHighlightBackground": "#ffa7474d", // theme orange deep at 30% opacity - Color of the other search matches
    "editor.findRangeHighlightBackground": "#3d3d3d4d", // theme gray-black-fade at 30% opacity - example: using finder widget with selection search only
    "editor.findMatchBackground": "#98bffe66", // theme blue at 40% opacity - example: current search match
    // match selection
    "editor.hoverHighlightBackground": "#98bffe1a", // theme blue at 10% opacity
    // ruler
    "editorRuler.foreground": "#ebb0771a", // theme orange at 10% opacity
    // scrollbar
    "editorOverviewRuler.background": "#242424", // 14% black (match editor bg)
    "editorOverviewRuler.border": "#333333", // 20% black (scrollbar border)
    "editorOverviewRuler.modifiedForeground": "#f4e4b899", // theme yellow at 60% opacity
    "editorOverviewRuler.addedForeground": "#9bce9099", // theme green at 60% opacity - example: scrollbar diff indicator
    "editorOverviewRuler.deletedForeground": "#ed8c8199", // theme red at 60% opacity
    "editorOverviewRuler.errorForeground": "#ed8c81e6", // theme red at 90% opacity
    "editorOverviewRuler.warningForeground": "#ebb077e6", // theme orange at 90% opacity
    "editorOverviewRuler.infoForeground": "#98bffee6", // theme blue at 90% opacity
    "editorOverviewRuler.bracketMatchForeground": "#7a7a7ae6", // theme gray dark at 90% opacity
    // errors and warnings
    "editorError.foreground":"#ed8c81", // theme red for error squiggly lines
    "editorHint.foreground": "#f4e4b8", // theme yellow for 3 dots underline
    "editorWarning.foreground": "#ebb077", // theme orange for warning squiggly lines
    "editorInfo.foreground": "#98bffe", // theme blue for info squiggly lines
    // unused code
    "editorUnnecessaryCode.opacity": "#00000099", // 60% opacity
    // selection
    // widget
    "editorWidget.foreground": "#cccccc", // theme light gray - example: the file search widget
    "editorWidget.background": "#333333", // #2e2e2e 18% black (match panel bg) - example: finder widget
    "editorWidget.border": "#3d3d3d", // theme gray-black-fade
    "editorWidget.resizeBorder": "#424242", // 26% black
    // word highlight
    "editor.wordHighlightBackground": "#525252", // theme gray fade (read-access, such as JS variable)
    "editor.wordHighlightStrongBackground": "#98bffe66", // theme blue at 40% opacity (write-access, such as JS variable)
    "editor.wordHighlightTextBackground": "#525252", // theme gray fade
    // unicode
    "editorUnicodeHighlight.border": "#ebb07780", // theme orange at 50% opacity
    "editorUnicodeHighlight.background": "#ebb0774d", // theme orange at 30% opacity

    //*** EDITOR GROUPS & TABS ***
    // groups
    "editorGroup.border": "#141414", // theme black-gray-black (split editor border)
    "editorGroup.dropBackground": "#f5f5f533", // theme gray white at 20% opacity
    "editorGroupHeader.noTabsBackground": "#292929", // theme gray black (match sidebar bg)
    "editorGroupHeader.tabsBackground": "#2e2e2e", // 18% black (2 shades lighter than sidebar bg)
    // tabs
    "tab.activeBackground": "#242424", // 14% black (match editor background)
    "tab.activeForeground": "#f5f5f5", // theme gray white
    "tab.border": "#242424", // 14% black (match editor background)
    "tab.unfocusedActiveBorderTop": "#242424", // 14% black (match editor background)
    "tab.lastPinnedBorder": "#141414", // theme black-gray-black (matches split view border)
    "tab.inactiveBackground": "#2e2e2e", // 18% black (match panels bg)
    "tab.inactiveForeground": "#a3a3a3", // theme gray medium
    "tab.unfocusedActiveForeground": "#a3a3a3", // theme gray medium
    "tab.unfocusedInactiveForeground": "#7a7a7a", // theme gray dark
    "tab.hoverForeground": "#f5f5f5", // theme gray white
    "tab.unfocusedHoverForeground": "#f5f5f5", // theme gray white
    // side-by-side borders
    "sideBySideEditor.horizontalBorder": "#141414", // theme black-gray-black
    "sideBySideEditor.verticalBorder": "#141414", // theme black-gray-black (see split settings with Default and User side by side)

    //*** GIT DECORATION ***
    "gitDecoration.addedResourceForeground": "#9bce90", // theme green - example: editor list when new resource is staged but not committed
    "gitDecoration.modifiedResourceForeground": "#f4e4b8", // theme yellow
    "gitDecoration.deletedResourceForeground": "#ed8c81", // theme red
    "gitDecoration.renamedResourceForeground": "#98bffe", // theme blue"
    "gitDecoration.stageModifiedResourceForeground": "#f4e4b8", // theme yellow
    "gitDecoration.stageDeletedResourceForeground": "#ed8c81", // theme red
    "gitDecoration.untrackedResourceForeground": "#9bce90", // theme green
    "gitDecoration.ignoredResourceForeground": "#858585", // 52% black (4 shades lighter than theme gray dark)
    "gitDecoration.conflictingResourceForeground": "#ebb077", // theme orange

    //*** INPUT CONTROL ***
    "input.background": "#3d3d3d", // theme gray-black-fade
    "input.border": "#424242", // 26% black (input and debug console border)
    "input.foreground": "#f5f5f5", // theme gray white
    "input.placeholderForeground": "#7a7a7a", // theme gray dark
    "inputOption.activeBackground": "#525252", // theme gray fade
    "inputOption.activeBorder": "#666666", // theme gray-fade-dark
    "inputOption.activeForeground": "#f4e4b8", // theme yellow

    //*** LISTS & TREES ***
    "list.activeSelectionBackground": "#ffffff33", // white at 20% opacity - example: settings UI dropdown selector
    "list.activeSelectionForeground": "#f5f5f5", // theme white - sidebar active item when moving with keyboard
    "list.focusAndSelectionOutline": "#fff0", // 0% opacity for no border showing
    "list.focusForeground": "#f5f5f5", // theme gray white
    "list.focusOutline": "#ffffff33", // white at 20% opacity
    "list.hoverBackground": "#ffffff0d", // white at 5% opacity
    "list.hoverForeground": "#f5f5f5", // theme white - example: extensions sidebar list
    "list.inactiveSelectionBackground": "#ffffff0d", // white at 5% opacity example: extensions sidebar list
    "list.inactiveSelectionForeground": "#f5f5f5", // theme gray white
    "list.errorForeground": "#ed8c81", // theme red - example: editor tab title
    "list.warningForeground": "#ebb077", // theme orange
    // trees
    "tree.indentGuidesStroke": "#3d3d3d", // theme gray-black-fade (sidebar explorer tree)
    "tree.tableColumnsBorder": "#333333", // 6 shades lighter than editor bg (see Keyboard Shortcuts page)
    "tree.tableOddRowsBackground": "#1f1f1f", // 2 shades darker than editor bg (see Keyboard Shortcuts page)

    //*** PANELS ***
    "panel.background": "#2e2e2e", // 18% black
    "panel.border": "#383838", // 22% black (match title bar bg)
    "panel.dropBorder": "#7a7a7a", // theme gray dark
    "panelTitle.activeBorder": "#2e2e2e", // 18% black (match panel BG to hide border)
    "panelTitle.activeForeground": "#f4e4b8", // theme yellow
    "panelTitle.inactiveForeground": "#7a7a7a", // theme gray dark

    //*** PEEK VIEW ***
    "peekView.border": "#3d3d3d", // theme gray-black-fade
    "peekViewTitle.background": "#333333", // 20% black (match activity bar bg)
    "peekViewTitleDescription.foreground": "#a3a3a3", // theme gray medium
    "peekViewTitleLabel.foreground": "#f5f5f5", // theme gray white

    //*** PROBLEMS PANEL ***
    "problemsErrorIcon.foreground": "#ed8c81", // problems panel icons
    "problemsWarningIcon.foreground": "#ebb077", // problems panel icons
    "problemsInfoIcon.foreground": "#98bffe", // problems panel icons

    //*** PROGRESS BAR ***
    "progressBar.background": "#9bce90", // theme green

    //*** SCROLLBAR CONTROL ***
    "scrollbar.shadow": "#1f1f1f", // 12% black - Two shades darker than editor background - example: editor tabs side scroll
    "scrollbarSlider.activeBackground": "#7a7a7a66", // theme gray-fade-dark at 40% opacity - example: when selecting and dragging scrollbar location
    "scrollbarSlider.background": "#7a7a7a33", // theme gray-fade-dark at 20% opacity - example: editor vertical scrollbar location
    "scrollbarSlider.hoverBackground": "#7a7a7a4d", // theme gray-fade-dark at 30% opacity

    //*** SETTINGS ***
    "settings.modifiedItemIndicator": "#f4e4b8", // theme yellow - example: vertical line color in Settings UI

    //*** SIDEBAR ***
    "sideBar.background": "#292929", // theme gray black
    "sideBar.foreground": "#cccccc", // theme gray light
    "sideBar.border": "#292929", // theme gray Black
    "sideBar.dropBackground": "#2e2e2e", // 18% black - 2 shades darker than sidebar bg
    "sideBarTitle.foreground": "#707070", // 44% black (match comments) - EXPLORER TITLEc
    "sideBarSectionHeader.background": "#333333", // 20% black (match activity bar bg)
    "sideBarSectionHeader.foreground": "#a3a3a3", // theme gray medium
    "sideBarSectionHeader.border": "#292929", // theme gray Black (match sidebar border)

    //*** STATUS BAR ***
    "statusBar.background": "#383838", // 22% black (match title bar bg)
    "statusBar.foreground": "#8f8f8f", // theme gray-dark-medium
    "statusBar.debuggingBackground": "#ebb077", // theme orange
    "statusBar.debuggingForeground": "#141414", // theme black-gray-black
    "statusBar.noFolderBackground": "#383838", // 22% black (match title bar bg)
    "statusBar.noFolderForeground": "#8f8f8f", // theme gray-dark-medium

    //*** TERMINAL ***
    "terminal.background": "#242424", // 14% black (match editor bg)
    "terminal.foreground": "#cccccc", // theme gray light
    "terminal.ansiBlack": "#3d3d3d", // theme gray-black-fade
    "terminal.ansiBlue": "#317fff", // theme blue deep
    "terminal.ansiBrightBlack": "#7a7a7a", // theme gray dark
    "terminal.ansiBrightBlue": "#98bffe", // theme blue
    "terminal.ansiBrightCyan": "#91e0ee", // theme cyan
    "terminal.ansiBrightGreen": "#47e200", // theme green deep
    "terminal.ansiBrightMagenta": "#febccf", // theme pink
    "terminal.ansiBrightRed": "#ff7f72", // theme red deep
    "terminal.ansiBrightWhite": "#ffffff", // pure white
    "terminal.ansiBrightYellow": "#ebb077", // theme orange
    "terminal.ansiCyan": "#52e7ff", // theme cyan deep
    "terminal.ansiGreen": "#9bce90", // theme green
    "terminal.ansiMagenta": "#d2acee", // theme purple
    "terminal.ansiRed": "#ed8c81", // theme red
    "terminal.ansiWhite": "#f5f5f5", // theme gray white
    "terminal.ansiYellow": "#f4e4b8", // theme yellow

    //*** TEXT COLORS ***
    "textBlockQuote.background": "#3d3d3d", // theme gray-black-fade - example: markdown > quote in preview mode
    "textBlockQuote.border": "#666666", // theme gray-fade-dark
    "textCodeBlock.background": "#525252", // theme gray fade - example: hover over settings json "workbench.startupEditor"
    "textLink.activeForeground": "#f4e4b8", // theme yellow
    "textLink.foreground": "#98bffe", // theme blue - example: Settings UI text link
    "textPreformat.foreground": "#ffe294", // theme yellow deep

    //*** TITLE BAR ***
    "titleBar.activeBackground": "#383838", // 22% black (match status bar bg)
    "titleBar.activeForeground": "#8f8f8f", // theme gray-dark-medium
    "titleBar.inactiveBackground": "#2e2e2e", // 18% black (match editor inactive tabs bg)
    "titleBar.inactiveForeground": "#7a7a7a", // theme gray dark

    //*** TOOLBAR ACTION COLORS ***
    "toolbar.hoverBackground": "#f5f5f51a", // theme gray white at 10% opacity - example: Settings UI gear icons
    "toolbar.activeBackground": "#f5f5f533", // theme gray white at 20% opacity - example: Settings UI gear icons

  },
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "class": {
      "foreground": "#91e0ee", // theme cyan
    },
    "function": {
      "foreground": "#91e0ee", // theme cyan
    },
    "function.defaultLibrary": {
      "foreground": "#f5f5f5", // theme gray white
    },
    "method": {
      "foreground": "#f5f5f5", // theme gray white
    },
    "namespace": {
      "foreground": "#d9a4ff", // theme purple deep
    },
    "parameter": {
      "foreground": "#ebb077", // theme orange
    },
    "property": {
      "foreground": "#9bce90", // theme green
    },
    "variable": {
      "foreground": "#febccf", // theme pink
    },
    "variable.defaultLibrary": {
      "foreground": "#cccccc", // theme gray light
    },
  },
  "tokenColors":[
    {
      "name": "BOLD",
      "scope": [
        //*** MARKDOWN ***
        "markup.bold.markdown",
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "ITALIC",
      "scope": [
        //*** DEFAULT ***
        "comment",
        "variable.language.this",
        //*** MARKDOWN ***
        "entity.name.section.markdown",
        "markup.italic.markdown"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "BOLD ITALIC",
      "scope": [
        //*** MARKDOWN ***
        "markup.bold.markdown markup.italic.markdown",
      ],
      "settings": {
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "STRIKETHROUGH",
      "scope": [
        //*** MARKDOWN ***
        "markup.strikethrough.markdown",
      ],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Theme Gray Fade (32%)",
      "scope": [
        //*** DEFAULT ***
        "invalid",
      ],
      "settings": {
        "foreground": "#525252" // theme gray fade
      }
    },
    {
      "name": "44% Black",
      "scope": [
        //*** DEFAULT ***
        "comment",
        "comment constant",
        "comment entity.name.type",
        "comment keyword",
        "comment meta.tag.inline",
        "comment punctuation",
        "comment storage",
        "comment storage.modifier",
        "comment storage.type",
        "comment support",
        "comment variable",
        "punctuation.definition.comment",
        "source.embedded",
      ],
      "settings": {
        "foreground": "#707070"
      }
    },
    {
      "name": "Theme Gray Dark (48%)",
      "scope": [
        //*** MARKDOWN ***
        "markup.underline.link",
      ],
      "settings": {
        "foreground": "#7a7a7a" // theme gray dark
      }
    },
    {
      "name": "Theme Gray Medium (64%)",
      "scope": [
        //*** DEFAULT ***
        "entity.name",
        "meta.at-rule.keyframes.body",
        "meta.block",
        "meta.brace",
        "meta.embedded.line",
        "punctuation",
        //*** CSS ***
        "meta.function.calc.css", // For nesting parentheses in calc function
        "meta.property-list",
        //*** MARKDOWN ***
        "meta.separator.markdown",
        "markup.strikethrough.markdown",
        //*** PHP ***
        "meta.embedded.block.php", // For else colon when using alternative syntax
        "meta.embedded.line.php", // For closing bracket } when opening a new PHP tag
        //*** RUBY ***
        "meta.function.method.with-arguments.ruby storage.type", // For Ruby's parameter block `&` operator
        //*** XML ***
        "meta.tag",
        //*** ZSH ***
        "meta.parameter-expansion",
      ],
      "settings": {
        "foreground": "#a3a3a3" // theme gray medium
      }
    },
    {
      "name": "Theme Gray Light (80%)",
      "scope": [
        //*** DEFAULT ***
        "entity.name.label",
        "support.constant.property", // semantic token type for JS
        "support.variable", // semantic token type for JS
        //*** HTML ***
        "entity.other.attribute-name",
        //*** JAVASCRIPT ***
        "variable.other.object", // semantic token type for JS
        //*** SHELL ***
        "meta.scope",
      ],
      "settings": {
        "foreground": "#cccccc" // theme gray light
      }
    },
    {
      "name": "Theme Gray White (96%)",
      "scope": [
        //*** DEFAULT ***
        "meta.method-call entity.name.function",
        "support.function",
        //*** APACHE ***
        "entity.helper.apacheconf", // Using Apache Conf extension for syntax grammar
        //*** CSS ***
        "entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element",
        //*** HTML ***
        "text.html",
        "meta.element.structure.svg.text.html",
        //*** XML ***
        "text.xml",
      ],
      "settings": {
        "foreground": "#f5f5f5" // theme gray white
      }
    },
    {
      "name": "Theme Blue",
      "scope": [
        //*** DEFAULT ***
        "storage.type",
        //*** APACHE ***
        "string.path.apacheconf",
        //*** CSS ***
        "meta.attribute.unrecognized.xmlns.html string",
        "meta.attribute.unrecognized.xmlns:xlink.html string",
        //*** HTML ***
        "meta.attribute.href string",
        "meta.attribute.src.html string",
        "meta.attribute.srcset.html string",
        //*** MARKDOWN ***
        "meta.link.reference",
        "meta.link.reference constant",
        "meta.link.reference string",
        "string.other.link.title.markdown",
      ],
      "settings": {
        "foreground": "#98bffe" // theme blue
      }
    },
    {
      "name": "Theme Blue at 50% opacity",
      "scope": [
        //*** MARKDOWN ***
        "markup.strikethrough.markdown string.other.link.title.markdown",
      ],
      "settings": {
        "foreground": "#98bffe80" // theme blue at 50% opacity
      }
    },
    {
      "name": "Theme Cyan",
      "scope": [
        //*** DEFAULT ***
        "entity.name.type.class",
        "entity.name.function",
        "keyword.other.special-method", // such as Ruby loop
        "meta.at-rule.media.header",
        "meta.attribute.style string",
        "meta.property-name",
        "meta.property-value support.function",
        "support",
        //*** APACHE ***
        "entity.tag.apacheconf",
        //*** CSS ***
        "meta.property-value",
        //*** INI ***
        "keyword.other.definition",
        //*** PHP ***
        "support.function.basic_functions",
        "entity.other.inherited-class.php", // extends class
        "entity.other.alias.php", // use as
        //*** YAML ***
        "entity.name.tag.yaml",
      ],
      "settings": {
        "foreground": "#91e0ee" // theme cyan
      }
    },
    {
      "name": "Theme Green",
      "scope": [
        //*** DEFAULT ***
        "entity.other.attribute-name.class",
        "meta.attribute.class string",
        "meta.object-literal",
        "support.variable.property", // semantic token type
        "variable.other.constant.property", // semantic token type
        "variable.other.property",
        "variable.other.object.property",
        //*** APACHE ***
        "entity.mime-type.apacheconf",
        //*** MARKDOWN ***
        "entity.name.section.markdown",
        "markup.heading.markdown string.other.link.title.markdown",
        "markup.heading.markdown meta.other.valid-ampersand.markdown",
        "punctuation.definition.heading.markdown",
        // PHP
        "variable.other.property.php punctuation.definition.variable.php",
      ],
      "settings": {
        "foreground": "#9bce90" // theme green
      }
    },
    {
      "name": "Theme Orange",
      "scope": [
        //*** DEFAULT ***
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.id punctuation",
        "meta.attribute.id string",
        "variable.parameter",
        //*** APACHE ***
        "string.replacement.apacheconf",
        //*** CSS ***
        "entity.other.keyframe-offset",
        "meta.at-rule.keyframes",
        //*** MARKDOWN ***
        //*** PHP ***
        "meta.function.parameters.php variable",
        "meta.function.parameters.php punctuation.definition.variable",
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php",
        //*** SCSS ***
        "meta.at-rule.keyframes entity.name.function",
        "entity.other.attribute-name.scss",
      ],
      "settings": {
        "foreground": "#ebb077" // theme orange
      }
    },
    {
      "name": "Theme Pink",
      "scope": [
        //*** DEFAULT ***
        "punctuation.definition.variable",
        "variable",
      ],
      "settings": {
        "foreground": "#febccf" // theme pink
      }
    },
    {
      "name": "Theme Purple",
      "scope": [
        //*** DEFAULT ***
        "constant",
        "keyword.other.unit",
        //*** APACHE ***
        "entity.status.apacheconf",
        //*** HTML ***
        "constant punctuation.definition.entity", // HTML symbol entity
